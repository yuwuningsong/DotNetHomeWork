685" target="_blank" data-report-query="spm=1000.2115.3001.4128" data-report-click="{&quot;spm&quot;:&quot;1000.2115.3001.4128&quot;,&quot;dest&quot;:&quot;https://edu.csdn.net/course/play/27685&quot;,&quot;extra&quot;:&quot;{\&quot;fId\&quot;:67,\&quot;fName\&quot;:\&quot;floor-recommend\&quot;,\&quot;compName\&quot;:\&quot;card-list\&quot;,\&quot;compDataId\&quot;:\&quot;recommend-special\&quot;,\&quot;fTitle\&quot;:\&quot;推荐专题\&quot;,\&quot;pageId\&quot;:18}&quot;}" data-report-view="{&quot;spm&quot;:&quot;1000.2115.3001.4128&quot;,&quot;dest&quot;:&quot;https://edu.csdn.net/course/play/27685&quot;,&quot;extra&quot;:&quot;{\&quot;fId\&quot;:67,\&quot;fName\&quot;:\&quot;floor-recommend\&quot;,\&quot;compName\&quot;:\&quot;card-list\&quot;,\&quot;compDataId\&quot;:\&quot;recommend-special\&quot;,\&quot;fTitle\&quot;:\&quot;推荐专题\&quot;,\&quot;pageId\&quot;:18}&quot;}" data-v-a4363172><div class="blog_wrap" data-v-a4363172><div class="blog" data-v-a4363172><img class="blog_img" data-v-a4363172> <div class="hover_mask2" data-v-a4363172></div> <!----> <!----> <!----> <div class="imgTextPlug_info" data-v-a4363172><!----></div></div> <div class="info" data-v-a4363172><p class="headP" data-v-a4363172>英特尔® OpenVINO™工具套件初级</p> <p class="infoP" data-v-a4363172>英特尔® OpenVINO™工具套件初级</p></div></div></a></div><div class="card_item_blog" data-v-a4363172 data-v-b158904c><a href="https://bss.csdn.net/m/topic/intel_edge?project_id=4522&amp;utm_source=BIZ_topic_HDRL" target="_blank" data-report-query="spm=1000.2115.3001.4128" data-report-click="{&quot;spm&quot;:&quot;1000.2115.3001.4128&quot;,&quot;dest&quot;:&quot;https://bss.csdn.net/m/topic/intel_edge?project_id=4522&amp;utm_source=BIZ_topic_HDRL&quot;,&quot;extra&quot;:&quot;{\&quot;fId\&quot;:67,\&quot;fName\&quot;:\&quot;floor-recommend\&quot;,\&quot;compName\&quot;:\&quot;card-list\&quot;,\&quot;compDataId\&quot;:\&quot;recommend-special\&quot;,\&quot;fTitle\&quot;:\&quot;推荐专题\&quot;,\&quot;pageId\&quot;:18}&quot;}" data-repo