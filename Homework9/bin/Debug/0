    var shopUserName = username;
    ;$(function(){
        window.csdn.shop.show({
            userName: shopUserName,
            topEl: $("#csdn-shop-window-top"),
            bottomEl: $("#csdn-shop-window"),
            callBack: function(res){
                if(!res.top){
